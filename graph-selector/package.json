    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-lookahead-lookbehind-regexp": "^0.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@types/cytoscape": "^3.19.11",
    "html-entities": "^2.4.0",
